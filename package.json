{
  "name": "trpg-game-client",
  "version": "0.5.6",
  "description": "专注于TRPG跑团的IM即时应用聊天软件",
  "main": "./dist/entry.js",
  "productName": "TRPG Engine",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run dev:main",
    "dev:pro": "cross-env NODE_ENV=production npm run dev:main",
    "dev:main": "cross-env NODE_OPTIONS='--max_old_space_size=4096' webpack-dev-server --progress --color --config ./build/config/webpack.config.js",
    "dev:proserver": "cross-env TRPG_HOST=trpgapi.moonrailgun.com:80 npm run dev",
    "dev:sandbox": "webpack-dev-server --config ./build/config/webpack.sandbox.config.js",
    "dev:sw": "cross-env DEV_SW=true npm run dev",
    "build:main": "cross-env NODE_OPTIONS='--max_old_space_size=4096' webpack --progress --color --config ./build/config/webpack.config.js",
    "build:dev": "cross-env PLATFORM=web npm run build:main",
    "build:pro": "cross-env NODE_ENV=production npm run build:main && npm run clean:sourcemap",
    "build:report": "cross-env npm_config_report=true npm run build:pro",
    "build:dll": "webpack --config build/config/webpack.dll.config.js",
    "smbrowser:dev": "cross-env NODE_ENV=development electron ./build/electron/entry.js",
    "smbrowser": "cross-env NODE_ENV=production electron ./build/electron/entry.js",
    "pc": "cross-env PLATFORM=electron NODE_ENV=production TRPG_HOST=localhost gulp --gulpfile build/gulpfile.js && electron .",
    "pcdev": "cross-env PLATFORM=electron npm run dev & gulp assets --gulpfile build/gulpfile.js && cross-env NODE_ENV=development electron .",
    "package": "cross-env PLATFORM=electron NODE_ENV=production gulp package --gulpfile build/gulpfile.js",
    "package-all": "cross-env PLATFORM=electron NODE_ENV=production gulp package:all --gulpfile build/gulpfile.js",
    "pc:builder": "cross-env PLATFORM=electron NODE_ENV=production gulp package:builder --gulpfile build/gulpfile.js",
    "portal:dev": "cross-env NODE_ENV=development NODE_OPTIONS='--max_old_space_size=2048' webpack-dev-server --progress --color --config ./build/config/webpack.portal.config.js",
    "portal:build": "cross-env NODE_OPTIONS='--max_old_space_size=4096' NODE_ENV=production webpack --progress --color --config ./build/config/webpack.portal.config.js && npm run clean:sourcemap",
    "playground:dev": "cross-env NODE_ENV=development NODE_OPTIONS='--max_old_space_size=4096' webpack-dev-server --progress --color --config ./build/config/webpack.playground.config.js",
    "playground:build": "cross-env NODE_ENV=production NODE_OPTIONS='--max_old_space_size=4096' webpack --progress --color --config ./build/config/webpack.playground.config.js && npm run clean:sourcemap",
    "clean:sourcemap": "rimraf ./dist/**/*.js.map",
    "translation:scan": "i18next-scanner --config build/config/i18next-scanner.config.js",
    "snowpack": "cross-env NODE_OPTIONS='--max_old_space_size=4096' snowpack dev --polyfill-node",
    "snowpack:build": "cross-env NODE_OPTIONS='--max_old_space_size=4096' snowpack build --polyfill-node",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prettier:all": "prettier --write \"./src/**/*.{ts,tsx,json,less}\" --config ./.prettierrc.json",
    "postinstall": "npm run install:all",
    "install:app": "cd ./src/app && npm install",
    "install:appv2": "cd ./src/appv2 && yarn",
    "install:web": "cd ./src/web && npm install",
    "install:all": "npm run install:app && npm run install:appv2 && npm run install:web",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version -a"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/*.{json,less}": [
      "prettier --write --config ./.prettierrc.json"
    ],
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write --config ./.prettierrc.json"
    ]
  },
