{
  "name": "trpg-game-client",
  "version": "0.3.4",
  "description": "专注于TRPG跑团的IM即时应用聊天软件",
  "main": "./dist/entry.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run dev:main",
    "dev:pro": "cross-env NODE_ENV=production npm run dev:main",
    "dev:main": "cross-env NODE_OPTIONS='--max_old_space_size=4096' webpack-dev-server --hot --inline --progress --colors --port 8089 --host 0.0.0.0 --config ./build/config/webpack.config.js",
    "dev:proserver": "cross-env TRPG_HOST=trpgapi.moonrailgun.com:80 npm run dev",
    "build:main": "webpack --progress --colors --display-modules --config ./build/config/webpack.config.js",
    "build:dev": "cross-env PLATFORM=web npm run build:main",
    "build:pro": "cross-env NODE_ENV=production ASSET_PATH=//static3.moonrailgun.com/ npm run build:main",
    "build:report": "cross-env npm_config_report=true npm run build:pro",
    "build:dll": "webpack --config build/config/webpack.dll.config.js",
    "pc": "cross-env PLATFORM=electron NODE_ENV=production TRPG_HOST=localhost gulp --gulpfile build/gulpfile.js && electron .",
    "pcdev": "cross-env PLATFORM=electron npm run dev & gulp assets --gulpfile build/gulpfile.js && cross-env NODE_ENV=development electron .",
    "package": "cross-env PLATFORM=electron NODE_ENV=production gulp package --gulpfile build/gulpfile.js",
    "package-all": "cross-env PLATFORM=electron NODE_ENV=production gulp package:all --gulpfile build/gulpfile.js",
    "pc:builder": "cross-env PLATFORM=electron NODE_ENV=production gulp package:builder --gulpfile build/gulpfile.js",
    "app": "cd ./src/app && npm run dev:main",
    "app:clear": "npm run app -- --reset-cache",
    "app:proserver": "cross-env TRPG_HOST=trpgapi.moonrailgun.com:80 npm run app",
    "app:proserver:clear": "cross-env TRPG_HOST=trpgapi.moonrailgun.com:80 npm run app:clear",
    "app:build:env": "node ./src/app/build/metro/build-env.js",
    "app:build:android": "cd ./src/app/android && npm run build:android",
    "app:bundle:android": "cross-env START_DIR='../../' PLATFORM=app REACT_NATIVE_APP_ROOT=. react-native bundle --platform android --entry-file index.js --bundle-output ./dist/index.android.bundle --dev false",
    "app:deploy:codepush": "cd ./src/app && npm run deploy",
    "app:test": "cd ./src/app && npm run test",
    "portal:dev": "cross-env NODE_ENV=development NODE_OPTIONS='--max_old_space_size=2048' webpack-dev-server --progress --color --config ./build/config/webpack.portal.config.js",
    "portal:build": "cross-env NODE_OPTIONS='--max_old_space_size=4096' NODE_ENV=production ASSET_PATH=//static3.moonrailgun.com/ webpack --progress --color --config ./build/config/webpack.portal.config.js",
    "playground:dev": "cross-env NODE_ENV=development NODE_OPTIONS='--max_old_space_size=4096' webpack-dev-server --progress --colors --config ./build/config/webpack.playground.config.js",
    "playground:build": "cross-env NODE_ENV=production ASSET_PATH=//static3.moonrailgun.com/ NODE_OPTIONS='--max_old_space_size=4096' webpack --progress --colors --config ./build/config/webpack.playground.config.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint --ext .js,.jsx src",
    "lint:fix": "eslint --fix --ext .js,.jsx src",
    "prettier:all": "prettier --write \"src/**/*.js\"",
    "postinstall": "npm run app:build:env && npm run install:all",
    "install:app": "cd ./src/app && npm install",
    "install:web": "cd ./src/web && npm install",
    "install:all": "npm run install:app && npm run install:web",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version -a"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TRPGEngine/Client.git"
  },
  "keywords": [
    "trpg"
  ],
  "author": "moonrailgun",
  "license": "GPL-3.0",
