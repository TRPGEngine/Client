{
  "name": "trpg-game-client",
  "version": "0.6.10",
  "description": "专注于TRPG跑团的IM即时应用聊天软件",
  "main": "./dist/entry.js",
  "productName": "TRPG Engine",
  "scripts": {
    "dev": "cross-env NODE_ENV=development yarn run dev:main",
    "dev:pro": "cross-env NODE_ENV=production yarn run dev:main",
    "dev:main": "cross-env NODE_OPTIONS='--max_old_space_size=4096' webpack serve --progress --color --config ./build/config/webpack.config.js",
    "dev:proserver": "cross-env TRPG_HOST=trpgapi.moonrailgun.com:80 yarn run dev",
    "dev:sandbox": "webpack serve --config ./build/config/webpack.sandbox.config.js",
    "dev:sw": "cross-env DEV_SW=true yarn run dev",
    "build:main": "cross-env NODE_OPTIONS='--max_old_space_size=4096' webpack --progress --color --config ./build/config/webpack.config.js",
    "build:dev": "cross-env PLATFORM=web yarn run build:main",
    "build:pro": "yarn run build:dependent && cross-env NODE_ENV=production yarn run build:main && yarn run clean:sourcemap",
    "build:report": "cross-env npm_config_report=true yarn run build:pro",
    "build:dll": "webpack --config build/config/webpack.dll.config.js",
    "build:ci": "cross-env CI=true NODE_ENV=production yarn run build:main --profile --json=./stats.json",
    "build:dependent": "yarn run plugins:all && yarn run translation:build",
    "smbrowser:dev": "cross-env NODE_ENV=development electron ./build/electron/entry.js",
    "smbrowser": "electron ./build/electron/entry.js",
    "smbrowser:build": "electron-builder -c ./build/config/builder.config.js",
    "pc": "cross-env PLATFORM=electron NODE_ENV=production TRPG_HOST=localhost gulp --gulpfile build/gulpfile.js && electron .",
    "pcdev": "cross-env PLATFORM=electron yarn run dev & gulp assets --gulpfile build/gulpfile.js && cross-env NODE_ENV=development electron .",
    "package": "cross-env PLATFORM=electron NODE_ENV=production gulp package --gulpfile build/gulpfile.js",
    "package-all": "cross-env PLATFORM=electron NODE_ENV=production gulp package:all --gulpfile build/gulpfile.js",
    "pc:builder": "cross-env PLATFORM=electron NODE_ENV=production gulp package:builder --gulpfile build/gulpfile.js",
    "portal:dev": "cross-env NODE_ENV=development NODE_OPTIONS='--max_old_space_size=2048' webpack serve --progress --color --config ./build/config/webpack.portal.config.js",
    "portal:build": "cross-env NODE_OPTIONS='--max_old_space_size=4096' NODE_ENV=production webpack --progress --color --config ./build/config/webpack.portal.config.js && yarn run clean:sourcemap",
    "playground:dev": "cross-env NODE_ENV=development NODE_OPTIONS='--max_old_space_size=4096' webpack serve --progress --color --config ./build/config/webpack.playground.config.js",
    "playground:build": "cross-env NODE_ENV=production NODE_OPTIONS='--max_old_space_size=4096' webpack --progress --color --config ./build/config/webpack.playground.config.js && yarn run clean:sourcemap",
    "clean:sourcemap": "rimraf ./dist/**/*.js.map",
    "translation:build": "node ./build/script/buildPublicTranslation.js",
    "translation:scan": "node ./build/script/scanTranslation.js",
    "presnowpack": "yarn run build:dependent",
    "snowpack": "cross-env NODE_OPTIONS='--max_old_space_size=4096' snowpack dev --polyfill-node",
    "snowpack:build": "cross-env NODE_OPTIONS='--max_old_space_size=4096' snowpack build --polyfill-node",
    "plugins:all": "ministar buildPlugin all",
    "plugins:watch:all": "ministar watchPlugin all",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prettier:all": "prettier --write \"./src/**/*.{ts,tsx,json,less}\" --config ./.prettierrc.json",
    "postinstall": "yarn run install:all",
    "install:app": "cd ./src/app && yarn",
    "install:appv2": "cd ./src/appv2 && yarn",
    "install:web": "cd ./src/web && yarn",
    "install:playground": "cd ./src/playground && yarn",
    "install:all": "yarn run install:appv2 && yarn run install:web && yarn run install:playground",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version -a -r patch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/*.{json,less}": [
      "prettier --write --config ./.prettierrc.json"
    ],
    "**/*.js": [
      "prettier --write --config ./.prettierrc.json"
    ],
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write --config ./.prettierrc.json"
    ]
  },
