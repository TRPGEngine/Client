{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom", "es7"],
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "jsx": "react",
    "strict": true,
    "skipLibCheck": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "importsNotUsedAsValues": "error",
  }
}
